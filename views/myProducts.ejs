			<!-- BEGIN PAGE CONTENT-->
			<div class="row">
				<div class="col-md-12">
					<!-- Begin: life time stats -->
					<div class="portlet">
						<div class="portlet-title">
							<div class="caption">
								<i class="fa fa-shopping-cart"></i>Listado de Productos
							</div>
							<div class="actions">
								<a href="/productEdit" class="btn default yellow-stripe">
								<i class="fa fa-plus"></i>
								<span class="hidden-480">
								Nuevo Producto </span>
								</a>
								<div class="btn-group">
									<a class="btn default yellow-stripe" href="#" data-toggle="dropdown">
									<i class="fa fa-share"></i>
									<span class="hidden-480">
									Herramientas </span>
									<i class="fa fa-angle-down"></i>
									</a>
									<ul class="dropdown-menu pull-right">
										<li>
											<a href="#">
											Exportar a Excel </a>
										</li>
										<li>
											<a href="#">
											Exportar a CSV </a>
										</li>
										<li>
											<a href="#">
											Exportar a XML </a>
										</li>
									</ul>
								</div>
							</div>
						</div>
						<div class="portlet-body" ng-controller="productController">
							<div class="table-container">
								<div class="table-actions-wrapper">
									<span>
									</span>
									<select class="table-group-action-input form-control input-inline input-small input-sm">
										<option value="">Seleccionar...</option>
										<option value="publish">Publicar</option>
										<option value="unpublished">Despublicar</option>
										<option value="delete">Eliminar</option>
									</select>
									<button class="btn btn-sm yellow table-group-action-submit"><i class="fa fa-check"></i> Aceptar</button>
								</div>
								<table class="table table-striped table-bordered table-hover">
								<thead>
								<tr role="row" class="heading">
									<th width="5%">
										<input type="checkbox" class="group-checkable">
									</th>
									<th width="15%">
										 Nombre
									</th>
									<th width="15%">
										 Descripcion
									</th>
									<th width="15%">
										 Categoria
									</th>
									<th width="15%">
										 Precio
									</th>
									<th width="10%">
										 Tipo De Impuesto
									</th>
									<th width="15%">
										 Fecha de Creacion
									</th>
									<th width="15%">
										 Estado
									</th>
									<th width="15%">
									</th>
								</tr>
								<tr role="row" class="filter" ng-repeat="product in products">
									<td>
									</td>
									<td>
										{{product.id}}
									</td>
									<td>
										{{product.name}}
									</td>
									<td>
										{{product.categoria}}
									</td>
									<td>
										{{product.price}}
									</td>
									<td>
										{{product.impuesto}}
									</td>
									<td>
										{{getDate(product.createdAt)}}
									</td>
									<td>
										{{product.status}}
									</td>
									<td>
										<a href="/product" class="btn btn-sm blue filter-cancel"><i class="fa fa-search"></i> Ver</a>
									</td>
								</tr>
								</thead>
								<tbody>
								</tbody>
								</table>
							</div>
						</div>
					</div>
					<!-- End: life time stats -->
				</div>
			</div>
